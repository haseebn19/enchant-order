{
  "name": "enchant-order",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "license": "AGPL-3.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint . && stylelint \"**/*.css\"",
    "lint:fix": "eslint . --fix && stylelint \"**/*.css\" --fix",
    "lint:css": "stylelint \"**/*.css\"",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "preview": "vite preview",
    "deploy": "tsc -b && cross-env BASE=/enchant-order/ vite build && gh-pages -d dist"
  },
  "dependencies": {
    "react": "^19.2.4",
    "react-dom": "^19.2.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@testing-library/react": "^16.0.1",
    "@types/node": "^22.10.0",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.4",
    "@vitest/coverage-istanbul": "^4.0.18",
    "eslint": "^9.39.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^17.3.0",
    "jsdom": "^25.0.1",
    "stylelint": "^17.3.0",
    "stylelint-config-standard": "^40.0.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.55.0",
    "vite": "^7.3.1",
    "vitest": "^4.0.18",
    "gh-pages": "^6.2.0",
    "cross-env": "^7.0.2"
  }
}
